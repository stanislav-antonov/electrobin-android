<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/common_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="20dp"
    android:paddingRight="20dp"
    android:background="#fff4f4d3"
    >
    <TextView
        android:id="@+id/header_text"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:textSize="20sp"
        android:textColor="#ff394445"
        android:layout_centerHorizontal="true"
        android:gravity="center_horizontal"
        android:layout_alignParentTop="true"
        android:layout_marginTop="30dp"
        />
    <LinearLayout
        android:id="@+id/values"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_above="@+id/new_route_button"
        android:layout_below="@+id/header_text"
        >
        <TableLayout
            android:id="@+id/table"
            android:layout_below="@id/header_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:stretchColumns="1"
            >
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >
                <TextView
                    android:text="Fill"
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:textSize="20sp"
                    android:textColor="#ff394445"
                    android:background="@drawable/table_cell_shape"
                    />
                <TextView
                    android:text="Count"
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:textSize="20sp"
                    android:textColor="#ff394445"
                    android:background="@drawable/table_cell_shape"
                    />
                <TextView
                    android:text="V"
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:textSize="20sp"
                    android:textColor="#ff394445"
                    android:background="@drawable/table_cell_shape"
                    />
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >
                <TextView
                    android:text="10 - 20"
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:textSize="16sp"
                    android:textColor="#ff607677"
                    android:background="@drawable/table_cell_shape"
                    />
                <TextView
                    android:text=""
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:textSize="16sp"
                    android:textColor="#ff607677"
                    android:background="@drawable/table_cell_shape"
                    />
                <TextView
                    android:text=""
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:textSize="16sp"
                    android:textColor="#ff607677"
                    android:background="@drawable/table_cell_shape"
                    />
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >
                <TextView
                    android:text="30 - 40"
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:textSize="16sp"
                    android:textColor="#ff607677"
                    android:background="@drawable/table_cell_shape"
                    />
                <TextView
                    android:text=""
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:textSize="16sp"
                    android:textColor="#ff607677"
                    android:background="@drawable/table_cell_shape"
                    />
                <TextView
                    android:text=""
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:textSize="16sp"
                    android:textColor="#ff607677"
                    android:background="@drawable/table_cell_shape"
                    />
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >
                <TextView
                    android:text="40 - 70"
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:textSize="16sp"
                    android:textColor="#ff607677"
                    android:background="@drawable/table_cell_shape"
                    />
                <TextView
                    android:text=""
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:textSize="16sp"
                    android:textColor="#ff607677"
                    android:background="@drawable/table_cell_shape"
                    />
                <TextView
                    android:text=""
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:textSize="16sp"
                    android:textColor="#ff607677"
                    android:background="@drawable/table_cell_shape"
                    />
            </TableRow>
            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >
                <TextView
                    android:text="70 - 100"
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:textSize="16sp"
                    android:textColor="#ff607677"
                    android:background="@drawable/table_cell_shape"
                    />
                <TextView
                    android:text=""
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:textSize="16sp"
                    android:textColor="#ff607677"
                    android:background="@drawable/table_cell_shape"
                    />
                <TextView
                    android:text=""
                    android:padding="10dp"
                    android:gravity="center_horizontal"
                    android:textSize="16sp"
                    android:textColor="#ff607677"
                    android:background="@drawable/table_cell_shape"
                    />
            </TableRow>
        </TableLayout>
        <TextView
            android:id="@+id/run_text"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:textSize="20sp"
            android:textColor="#ff394445"
            android:gravity="center_horizontal"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="30dp"
            />
    </LinearLayout>
    <Button
        android:id="@+id/new_route_button"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="30dp"
        android:textSize="20sp"
        android:textColor="#FFFFFFFF"
        android:textAllCaps="false"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:background="@drawable/custom_round_button_1"
        style="?android:attr/borderlessButtonStyle"
        />
</RelativeLayout>
